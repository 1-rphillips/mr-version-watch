<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.rphillips.mrversionwatch</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/bash</string>
            <string>/Library/Networks/MRVersionWatch.sh</string>
        </array>
        <key>KeepAlive</key>
        <false/>
        <key>StartCalendarInterval</key>
    	<dict>
	        <key>Minute</key>
	        <integer>0</integer>
	        <key>Hour</key>
	        <integer>9</integer>
	    </dict>
    </dict>
</plist>